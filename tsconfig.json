{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES6", "DOM", "ES2017.Object"],
    "allowJs": false,
    "outDir": "./build",
    "strict": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react"
  }
}
