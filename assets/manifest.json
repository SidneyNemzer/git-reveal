{
  "manifest_version": 2,
  "name": "Pages2Repo",
  "version": "1",
  "permissions": ["webRequest", "https://*/*", "http://*/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "checking.html",
    "default_icon": "icon-black.png"
  }
}
